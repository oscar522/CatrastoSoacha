
@{
    ViewBag.Title = "ListarUsuarios";
}

<h2>Listar Usuarios</h2>
<table id="usersTbl" class="table table-striped cell-border compact stripe" style="width:100%">
</table>
@section Scripts{
    @Scripts.Render("~/Scripts/Paginas/Account/ConsultarUsuarios.js")
<script>
    $(document).ready(function () {
        ConsultarUsuariosJs.urlListadoUsuarios = '@Url.Action("ListarUsuariosJson")';
        ConsultarUsuariosJs.urlActualizarUsuario = '@Url.Action("ActualizarUsuario")';
        ConsultarUsuariosJs.Inicializar();
    });
</script>
}