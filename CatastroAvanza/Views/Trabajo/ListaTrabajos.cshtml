
@{
    ViewBag.Title = "Lista Trabajos";
}

<div class="card mb-4 wow fadeIn">

    <!--Card content-->
    <div class="card-body d-sm-flex justify-content-between">
        <h4 class="mb-2 mb-sm-0 pt-1">
            <span>Lista Trabajos</span>
        </h4>
    </div>
</div>

<div class="wow fadeIn">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5> <label class="control-label col-md-12">Trabajos</label> </h5>
                                    <hr />
                                    <table class="table table-striped cell-border compact responsive nowrap" id="tbltrabajos" width="90%">
                                    </table>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    @Scripts.Render("~/Scripts/ModalControl.js")
    @Scripts.Render("~/Scripts/Paginas/Trabajo/TrabajoConsulta.js")
<script>
        $(document).ready(function () {
            TrabajoConsultaJs.urlObtenerTrabajos = '@Url.Action("ConsultarTrabajosUsuarios")';
            TrabajoConsultaJs.urlAgregarGestionTrabajo = '@Url.Action("GestionarTrabajo")';
            TrabajoConsultaJs.Inicializar();
        });
</script>

}