@section Scripts
{
    <script type="text/javascript">
        $(document).ready(function () {

           $( "#Btn" ).click(function() {
               $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetData")',
                    dataType: 'json',
                    data: { Id: $('#IdIn').val() },
                   success: function (s) {
                       var id = s.NUMERO_DEL_PREDIO;
                       var Zona_ = id.substring(0, 2);
                       var Sect_ = id.substring(2, 4);
                       var Com_ = id.substring(4, 6);
                       var Barr_ = id.substring(6, 8);
                       var Mz_ = id.substring(8, 12);
                       var Predio_ = id.substring(12, 16);
                       var Con_ = id.substring(16, 17);
                       var To_ = id.substring(17, 19);
                       var Piso_ = id.substring(19, 21);
                       var Unidad_ = id.substring(21, 25);
                       $("#Dep_").html(s.dep);
                       $("#Mun_").html(s.mun);
                       $("#Zona_").html(Zona_);
                       $("#Sect_").html(Sect_);
                       $("#Com_").html(Com_);
                       $("#Barr_").html(Barr_);
                       $("#Mz_").html(Mz_);
                       $("#Predio_").html(Predio_);
                       $("#Con_").html(Con_);
                       $("#To_").html(To_);
                       $("#Piso_").html(Piso_);
                       $("#Unidad_").html(Unidad_);
                       $("#DepNombre").html(s.R2_2021_69295_CONSTRUCCIONESModel[0].nomDep);
                       $("#NumNombre").html(s.R2_2021_69295_CONSTRUCCIONESModel[0].nomMun);

                       $("#Urb_Rural_").html();
                       $("#Condicion_").html();
                       $("#Comuna_").html(s.R1_2020_66069_PREDIOSModel[0].COMUNA);
                       $("#Mejoras_").html();
                       $("#Vereda_").html();
                       $("#Destino_").html(s.R1_2020_66069_PREDIOSModel[0].DESCRIPCION_DESTINO);
                       $("#Barrio_").html();
                       $("#Area_de_terreno_").html(s.R1_2020_66069_PREDIOSModel[0].AREA_TERRENO);
                       $("#Direccion_").html(s.R1_2020_66069_PREDIOSModel[0].DIRECCION);
                       $("#Area_Construida_").html(s.R1_2020_66069_PREDIOSModel[0].AREA_CONSTRUIDA);
                       $("#Direccion_2_").html();
                       $("#Avaluo_").html(s.R1_2020_66069_PREDIOSModel[0].AVALUO);

                       $("#FMI_").html(s.R2_2021_69295_CONSTRUCCIONESModel[0].FMI);
                       var PropietariosItem = "";
                       s.CANTIDAD_PROPIETARIOS.forEach(function (item) {
                           PropietariosItem = PropietariosItem + "<br>" + item
                       });
                        $("#Propietarios_").html(PropietariosItem);

                        // urbano rutal
                        if ( Zona_ == '01') {
                            $("#Urb_Rural_").html('Urbano');
                        } else if ( Zona_ == '00') {
                            $("#Urb_Rural_").html('Rural');
                        } else if ( Zona_ == '02') {
                            $("#Urb_Rural_").html('Corregimiento');
                        }

                        if ( To_ == '00') {
                            $("#Condicion_").html('NPH');
                            $("#Mejoras_").html('no');
                        } else if ( To_ == '05') {
                            $("#Condicion_").html('Mejora');
                            $("#Mejoras_").html('si');


                        } else if ( To_ == '09') {
                            $("#Condicion_").html('PH');
                            $("#Mejoras_").html('no');
                        } else if ( To_ == '08') {
                            $("#Condicion_").html('PH');
                            $("#Mejoras_").html('no');
                        }

                        $("#MejorasCantidad_").html(s.CANTIDAD_MEJORA);



                        console.log(s);
                },
                error: function(e){
                }
            });
            });


 });
    </script>
}